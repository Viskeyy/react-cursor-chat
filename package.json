{
  "name": "@yomo/react-cursor-chat",
  "version": "0.2.1",
  "description": "Cursor Chat like Figma",
  "keywords": [
    "presencejs",
    "yomo",
    "webtransport",
    "collaborative component",
    "realtime"
  ],
  "license": "MIT",
  "files": [
    "dist",
    "types.d.ts"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.esm.js",
  "typings": "./types.d.ts",
  "scripts": {
    "build:css": "npx tailwindcss -i ./style.css -o ./dist/style.css --minify",
    "build:js": "rollup -c --bundleConfigAsCjs",
    "build": "run-p build:js build:css",
    "watch:css": "npx tailwindcss -i ./style.css -o ./dist/style.css --watch",
    "watch:js": "rollup -c -w --bundleConfigAsCjs",
    "watch": "run-p watch:js watch:css",
    "start": "vite"
  },
  "types": "./types.d.ts",
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@faker-js/faker": "^7.6.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-replace": "^5.0.2",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@yomo/presence": "^2.2.0",
    "acorn-jsx": "^5.3.2",
    "autoprefixer": "^10.4.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "react": "^18.2.0",
    "react-dom": "18.1.0",
    "rollup": "^3.22.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^5.0.4",
    "vite": "^4.3.8"
  },
  "peerDependencies": {
    "@yomo/presence": "^2.2.0"
  }
}
